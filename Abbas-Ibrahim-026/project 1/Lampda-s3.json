{
    "AWSTemplateFormatVersion": "2010-09-09",     
    "Description": "A template to provision lambda and s3 bucket",
    "Resources": {
        "MyFirstLambdaFunction": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
                "FunctionName": "HarbhazLambdaFT",
 
                "Code": {
                                 "S3Bucket"  : "cilharbhaz",
                                 "S3Key"  :  "index.zip"
                },
                "Description": "Lambda function with cloudformation",
                "Handler": "index.lambda_handler",
                "Role": "arn:aws:iam::505458797783:role/S3PutObjectRole",
                "Runtime": "python3.9",
                "Timeout": 120
            }
        
        }
    }

}
